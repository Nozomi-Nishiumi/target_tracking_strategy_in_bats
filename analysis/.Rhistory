continuous<-function(data){
if(length(data)==1){return (list(data))}
bool=0
result<-numeric()
for(i in 1:(length(data)-1)){
if(data[i]&bool==0){
result=c(result,TRUE)
}else{
bool=bool+1
}
}
return(result)
}
